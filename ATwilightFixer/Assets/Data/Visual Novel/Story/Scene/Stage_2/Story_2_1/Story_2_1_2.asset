  sentences:
  - text: "~\u266A"
    speaker: {fileID: 11400000, guid: c59fe0cc86d3b554ab55d8fe4bd55b70, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC5EC\uAE30\uAC00 \uBBF8\uAD81 \uB3C4\uC2DC \uB8EC\uC2A4\uD2F0\uC544!"
    speaker: {fileID: 11400000, guid: c59fe0cc86d3b554ab55d8fe4bd55b70, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC640\uC544\u2026 \uB2E4\uC591\uD55C \uBCF5\uC7A5\uC758 \uC0AC\uB78C\uB4E4\u2026
      \uB300\uBD80\uBD84 \uBAA8\uD5D8\uAC00\uB4E4\uC774\uACA0\uC9C0?[n]\uADF8\uB7F0\uB370
      \uB8E8\uB4DC\uB294 \uC5B4\uB528\uC9C0?"
    speaker: {fileID: 0}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC124\uB9C8\u2026[n]\uD3B8\uC9C0\uB97C \uC548 \uC77D\uC5C8\uAC70\uB098\u2026?[n]\uADF8\uB7EC\uBA74
      \uC880 \uC11C\uC6B4\uD55C\uB370\u2026"
    speaker: {fileID: 0}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC774 \uBBF8\uCE5C\uB144\uC544! \uC11C\uC6B4\uD558\uAE34![n]\uD3B8\uC9C0
      \uBCF4\uB0B4\uACE0 \uBC14\uB85C \uCD9C\uBC1C\uD55C \uC0AC\uB78C\uC774 \uD3B8\uC9C0
      \uB3C4\uCC29\uC77C\uC740 \uC0DD\uAC01\uB3C4 \uC548\uD558\uACE0 \uC624\uB0D0?"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
      spriteIndex: 0
      actionType: 1
      coords: {x: 400, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC640\uC544~\u266A \uB8E8\uB4DC \uC624\uB79C\uB9CC\uC774\uB2E4. ~\u266A"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
      spriteIndex: 0
      actionType: 1
      coords: {x: -400, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC544\uC9C1\uB3C4 \uB9C8\uBC95\uC73C\uB85C \uC74C\uD45C \uB9CC\uB4E4\uC5B4\uB0B4\uACE0
      \uB2E4\uB2C8\uB0D0? \uBB34\uCE5C\uB828\u2026"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC65C\uC5D0\u2026 \uC774\uAC8C \uC5D8\uD504\uC758 \uAC10\uC815 \uD45C\uD604
      \uBC29\uC2DD\uC774\uAC70\uB4E0?"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC5D8\uD504\uB294 \uAC1C\uBFD4 \uB108\uB124 \uD560\uBA38\uB2C8\uAC00
      \uCFFC\uD130 \uC5D8\uD504\uC600\uB358 \uAC70\uC9C0 \uB10C \uADF8\uB0E5 \uC778\uAC04\uC774\uAC70\uB4E0\uC694?  "
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uADF8\uB798\uB3C4 \uB0B4 \uBAB8\uC5D0 16\uBD84\uC758 1\uC740 \uC5D8\uD504\uC758
      \uD53C\uAC00 \uD750\uB974\uACE0 \uC788\uC73C\uB2C8 \uB09C \uC5D8\uD504\uB77C\uACE0~\u266A"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC774 \uC815\uC2E0\uB098\uAC04 \uC5D8\uD504 \uD638\uC18C\uC778\uC774\u2026"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "~\u266A"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uD558\uC544\u2026[n]\uADF8\uB798 \uC774\uBBF8 \uC640\uBC84\uB838\uC73C\uB2C8\uAE4C\u2026"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uBAA8\uD5D8\uAC00 \uAE38\uB4DC\uB85C \uAC00\uC790"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC640\uC544~\u266A"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions: []
