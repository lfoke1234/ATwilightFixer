  sentences:
  - text: "\uC138\uC774\uD504!!!"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
      spriteIndex: 0
      actionType: 1
      coords: {x: 700, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uD760\u2026 \uC545\uB839\uC740 \uC0C1\uB300\uD558\uAE30 \uD798\uB4E4\uAD70."
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
      spriteIndex: 0
      actionType: 1
      coords: {x: 300, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uD798\uB4E4\uAE34! \uB0B4\uAC00 \uB2E4 \uD574\uCE58\uC6E0\uC796\uC544!"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC5B4\uADF8\uB85C \uC798 \uB04C\uC9C0 \uC54A\uC558\uB098?"
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uADF8\uB798\uC11C \uB54C\uB9AC\uC9C0\uB3C4 \uBABB\uD558\uB294 \uC545\uB839
      \uC5B4\uADF8\uB85C \uB04C\uC5B4\uC11C \uBE48\uC0AC \uC0C1\uD0DC\uAC00 \uB418\uC168\uC5B4\uC694?"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uB098\uB294 \uB178\uB825\uD588\uB2E4."
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC5B4\uD734\u2026 \uC811\uC218\uC6D0\uC528! \uD50C\uB808\uC544\uAC00
      \uBBF8\uAD81\uC5D0\uC11C \uC870\uB09C \uC0C1\uD0DC\uC785\uB2C8\uB2E4!"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uB124? \uC544\uAE4C\uBD80\uD130 \uBBF8\uAD81\uC774 \uC6B8\uB9B0\uB2E4
      \uC2F6\uC5C8\uB294\uB370\u2026"
    speaker: {fileID: 11400000, guid: 8d960552c86c720459d7e1cef3afb705, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 8d960552c86c720459d7e1cef3afb705, type: 2}
      spriteIndex: 0
      actionType: 1
      coords: {x: -650, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uAE38\uB4DC\uC7A5\uC5D0\uAC8C \uBCF4\uACE0 \uD574\uC8FC\uC2DC\uACE0 \uC9C0\uAE08
      \uB3C4\uC2DC\uC5D0 \uC788\uB294 \uBCA0\uD14C\uB791\uB4E4 \uBD88\uB7EC \uC8FC\uC138\uC694.
      \uC800\uB294 \uC7A5\uBE44\uB791 \uBB3C\uC790 \uC880 \uCC59\uAE30\uACE0 \uC62C
      \uAC8C\uC694. \uBD80\uD0C1\uB4DC\uB9BD\uB2C8\uB2E4."
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC800\uAE30\u2026 \uB8E8\uB4DC\u2026\uB2D8\u2026"
    speaker: {fileID: 11400000, guid: 8d960552c86c720459d7e1cef3afb705, type: 2}
