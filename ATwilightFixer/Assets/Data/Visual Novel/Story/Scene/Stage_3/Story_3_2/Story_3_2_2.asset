  sentences:
  - text: "\uD06C\uC74D\u2026"
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
      spriteIndex: 0
      actionType: 1
      coords: {x: 0, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uBCC4 \uAC83\uB3C4 \uC544\uB2CC \uBAAC\uC2A4\uD130\uB4E4 \uC8FC\uC81C\uC5D0\u2026
      \uD558\uC544\u2026"
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uB0B4\uAC00 \uC774\uB7F0 \uC800\uAE09\uD55C \uD568\uC815\uC5D0 \uAC78\uB9AC\uB2E4\uB2C8.
      \uD55C\uC2EC\uC2A4\uB7FD\uAD70"
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uACE0\uC791 \uC2AC\uB77C\uC784\uC778\uAC00\u2026 \uB0B4 \uB9C8\uC9C0\uB9C9\uC740\u2026"
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "...\uB9AC\u2026\uC2A4..."
    speaker: {fileID: 11400000, guid: c59fe0cc86d3b554ab55d8fe4bd55b70, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uADF8\uB798\uB3C4 \uBCF4\uD1B5 \uC2AC\uB77C\uC784\uC774 \uC544\uB2CC
      \uAC83\uC774 \uB2E4\uD589\uC778\uAC00. \uC2AC\uB77C\uC784\uC740 \uB9CE\uC774
      \uCC98\uB9AC\uD574 \uBD24\uC9C0\uB9CC \uB108\uC640 \uAC19\uC740 \uB140\uC11D\uC740
      \uCC98\uC74C \uBCF8\uB2E4."
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC8FD\uB354\uB77C\uB3C4 \uD63C\uC790 \uC8FD\uC9C0\uB294 \uC54A\uACA0\uB2E4.
      \uC8FD\uC5B4\uB77C \uC2AC\uB77C\uC784!"
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uCCA0\uD37D"
    speaker: {fileID: 11400000, guid: c59fe0cc86d3b554ab55d8fe4bd55b70, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uCC3E\uC558\uB2E4. \uC728\uB9AC\uC6B0\uC2A4!"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
      spriteIndex: 0
      actionType: 3
      coords: {x: -600, y: 0}
      moveSpeed: 2
    - speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
      spriteIndex: 0
      actionType: 1
      coords: {x: 400, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\u2026"
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC5B4 \uBB50\uC57C \uC774 \uC2AC\uB77C\uC784\uB4E4\uC740"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\u2026\uB8E8\uB4DC \uBB34\uC2A8\uC77C\uB85C \uB0A0 \uCC3E\uC558\uC9C0?"
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uBBF8\uAD81\uC774 \uC2EC\uC0C1\uCE58 \uC54A\uC544\uC11C \uBB34\uC2A8
      \uC77C\uC774 \uBC8C\uC5B4\uC9C8 \uAC70 \uAC19\uC544\uC11C \uB370\uB9AC\uB7EC
      \uC654\uC9C0."
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uB0A0\u2026 \uB370\uB9AC\uB7EC \uC654\uB2E4\uACE0? \uC65C\uC9C0?"
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uADF8\uC57C\u2026 \uBD88\uC548\uD558\uB2C8\uAE4C"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uB0B4\uAC00 \uBABB\uBBF8\uB365\uB2E4\uB294 \uAC83\uC774\uB0D0?"
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uB0B4\uAC00 \uBCF4\uAE30\uC5D0 \uBAA8\uD5D8\uAC00\uB85C \uB108\uB294
      \uC880 \uC560\uB9E4\uD574\uC11C \uB9D0\uC774\uC57C"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uAC10\uD788 \uADF8\uB7F0 \uB9D0\uC744!"
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC9C0\uAE08\uB3C4 \uBD10. \uC774\uB7F0 \uBCC4\uAC83\uB3C4 \uC544\uB2CC
      \uBAAC\uC2A4\uD130. \uC544 \uC8FD\uC9C0 \uC54A\uACE0 \uBD84\uC5F4\uD574\uC11C
      \uB3C4\uB9DD\uAC14\uB124. \uBB50 \uADF8\uB798\uBD24\uC790 \uC2AC\uB77C\uC784\uC774\uC9C0\uB9CC.[n]\uBCC4\uAC83\uB3C4
      \uC544\uB2CC \uD568\uC815\uC5D0 \uB2F9\uD588\uC796\uC544."
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
