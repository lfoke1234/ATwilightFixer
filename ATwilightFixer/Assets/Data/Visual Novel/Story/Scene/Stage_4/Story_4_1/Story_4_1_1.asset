  m_Name: Story_4_1_1
  m_EditorClassIdentifier: 
  sentences:
  - text: "\uC5B4\uB514\uBCF4\uC790\u2026 \uBBF8\uAD81\uC5D0 \uBB34\uC2A8 \uC7A5\uBE44\uB97C
      \uD558\uACE0 \uAC00\uC57C\uD560\uAE4C\u2026"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
      spriteIndex: 0
      actionType: 1
      coords: {x: 0, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uD760\u2026 \uD568\uC815\uC744 \uBE60\uB974\uAC8C \uD53C\uD560 \uC218
      \uC788\uAC8C \uC18D\uB3C4\uB97C \uCC59\uAE38\uAE4C\u2026 \uD568\uC815\uC774\uB098
      \uBAAC\uC2A4\uD130\uB97C \uCC98\uB9AC\uD560 \uACF5\uACA9\uB825\uC744 \uCC59\uAE38\uAE4C\u2026[n]\uCC28\uB77C\uB9AC
      \uC608\uC0C1\uCE58 \uBABB\uD55C \uACF5\uACA9\uC5D0 \uB9DE\uC544\uB3C4 \uBC84\uD2F8
      \uC218 \uC788\uB294 \uBC29\uC5B4\uB825? \uACE0\uBBFC\uB418\uB124"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uB8E8\uB4DC!!!"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
      spriteIndex: 0
      actionType: 3
      coords: {x: 400, y: 0}
      moveSpeed: 1
    - speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
      spriteIndex: 0
      actionType: 1
      coords: {x: -600, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC5B4? \uD50C\uB808\uC544 \uBB34\uC2A8\uC77C\uC774\uC57C?"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC624\uB298\uB3C4 \uBBF8\uAD81 \uAC08 \uAC70\uC9C0? \uB098\uB3C4 \uB370\uB824\uAC00\uC918\u266A"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC548\uB3FC. \uC5B4\uC81C \uBCF4\uB2C8\uAE4C \uC704\uD5D8\uD574 \uBCF4\uC774\uB354\uB77C."
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: ": \uADF8\uB7EC\uBA74 \uD63C\uC790 \uB4E4\uC5B4\uAC00\uAC8C?"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uAE38\uB4DC \uB9C8\uC2A4\uD130\uB4E4\uC740 \uC548 \uB3C4\uC640\uC8FC\uACE0
      \uBCA0\uD14C\uB791\uB4E4\uC740 \uB2E4\uB4E4 \uB2E4\uB978 \uB3C4\uC2DC\uB85C
      \uB5A0\uB098\uC11C \uC704\uD5D8\uD55C\uB370 \uD63C\uC790 \uAC00\uC57C\uC9C0."
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uADF8\uB798\uB3C4 \uD63C\uC790 \uBCF4\uB2E8 \uB458\uC774 \uB0AB\uC9C0
      \uC54A\uC744\uAE4C?"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC5D0\uC774. \uC5B4\uC81C \uBAA8\uD5D8\uAC00\uAC00 \uB41C \uC0DD\uCD08\uC9DC\uB97C
      \uB370\uB9AC\uACE0 \uC5B4\uB5BB\uAC8C \uB4E4\uC5B4\uAC00"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uD760\uD760\u266A \uB098 \uC544\uCE74\uB370\uBBF8 \uC218\uC11D \uB9C8\uBC95\uC0AC\uC778\uB370?
      \uC804\uD22C\uB3C4 \uD0D0\uC0C9\uB3C4 \uBAA8\uB450 \uB3C4\uC640\uC904 \uC218
      \uC788\uC5B4!"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC624\u2026"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC548\uB3FC."
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC65C\uC560\uC560\uC560\u2026 \uB098\uB3C4 \uAC08\uB798 \uC5B4\uC81C
      \uAC14\uB358 \uC7A5\uC18C\uB77C\uB3C4 \uAD1C\uCC2E\uC73C\uB2C8\uAE4C! \uAC70\uAE30\uC5D0\uC11C
      \uAD6C\uACBD\uB9CC \uD558\uACE0 \uC788\uC744 \uAC8C."
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\u2026\uBBF8\uAD81 \uC2DD\uBB3C\uC774 \uADF8\uB807\uAC8C \uAD81\uAE08\uD558\uB0D0?"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC5C4\uCCAD \uAD81\uAE08\uD558\uC9C0\u266A \uC870\uAE08\uB9CC \uB354
      \uC788\uC5C8\uC73C\uBA74 \uBAA9\uC18C\uB9AC\uB3C4 \uB4E4\uC744 \uC218 \uC788\uC5C8\uB294
      \uAC78?"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC2DD\uBB3C\uC758 \uBAA9\uC18C\uB9AC? \uBBF8\uAD81\uC5D0\uC11C\uB3C4
      \uB4E4\uC744 \uC218 \uC788\uC5B4?"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC2DC\uAC04\uB9CC \uC788\uC73C\uBA74 \uB4E4\uC744 \uC218 \uC788\uC5B4!
      \uC774\uAC74 \uD655\uC2E4\uD574\u266A"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uADF8\uB798? \uC11C\uB85C \uB300\uD654\uB3C4 \uD560 \uC218 \uC788\uC5B4?"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uADF8\uB7FC\u266A \uB2F9\uC5F0\uD558\uC9C0."
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC624\uCF00\uC774. \uADF8\uB7FC \uAC19\uC774 \uB4E4\uC5B4\uAC00\uC790.
      \uB098 \uD0D0\uC0C9\uD558\uB294 \uB3D9\uC548 \uC2DD\uBB3C \uC790\uC0DD\uC9C0\uC5D0\uC11C
      \uC2DD\uBB3C\uB4E4\uACFC \uC774\uC57C\uAE30 \uC880 \uB098\uB220\uBD10.[n]\uBBF8\uAD81\uC774
      \uC65C \uC774 \uBAA8\uC591\uC778\uC9C0\uB3C4 \uC880 \uBB3C\uC5B4 \uBD10\uC8FC\uACE0."
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC557\uC2F8\u266A"
