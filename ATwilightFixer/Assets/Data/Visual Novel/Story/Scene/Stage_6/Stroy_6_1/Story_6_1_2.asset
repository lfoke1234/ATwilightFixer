  m_EditorClassIdentifier: 
  sentences:
  - text: "\uC728\uB9AC\uC6B0\uC2A4\uB2D8! \uBBF8\uAD81 \uC785\uC7A5\uC740 \uC808\uB300
      \uC548\uB429\uB2C8\uB2E4!"
    speaker: {fileID: 11400000, guid: 8d960552c86c720459d7e1cef3afb705, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 8d960552c86c720459d7e1cef3afb705, type: 2}
      spriteIndex: 0
      actionType: 1
      coords: {x: -650, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC798 \uC54C\uACE0\uC788\uB124. \uBBF8\uAD81\uC758 \uD574\uACB0\uC0AC\uC758
      \uD30C\uD2F0\uC5D0 \uCD08\uB300\uBC1B\uC9C0 \uBABB\uD588\uC73C\uB2C8 \uB9D0\uC774\uC9C0."
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
      spriteIndex: 0
      actionType: 1
      coords: {x: 400, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uADF8\uAC83 \uB9D0\uACE0\uB3C4 \uC9C0\uAE08 \uBBF8\uAD81\uC740 \uB9E4\uC6B0
      \uC704\uD5D8\uD55C \uC2DC\uAE30\uC778 \uAC83 \uC54C\uACE0 \uACC4\uC2DC\uC796\uC544\uC694.
      \uADF8\uB9AC\uACE0 \uBC14\uB85C \uC5B4\uC81C! \uBB34\uB2E8 \uCE68\uC785 \uD558\uC168\uC796\uC544\uC694."
    speaker: {fileID: 11400000, guid: 8d960552c86c720459d7e1cef3afb705, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uADF8\uAC83 \uCC38 \uD560 \uB9D0\uC774 \uC5C6\uAD70."
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\u2026\uD750\uC74C"
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uD639\uC2DC \uB2E4\uB978 \uC6A9\uBB34\uAC00 \uC788\uC73C\uC2E0\uAC00\uC694?"
    speaker: {fileID: 11400000, guid: 8d960552c86c720459d7e1cef3afb705, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC5B4\uC81C \uBBF8\uAD81\uC5D0 \uB4E4\uC5B4\uAC00\uC11C \uB290\uB080
      \uAC83\uC774 \uB9CE\uC558\uB2E4. \uADF8\uB9AC\uACE0 \uBB34\uB2E8 \uCE68\uC785\uC73C\uB85C
      \uD53C\uD574\uB97C \uC900 \uAC83\uC5D0 \uB300\uD574 \uB2E4\uC2DC \uC0AC\uACFC\uD558\uC9C0."
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uBCF4\uB2F5\uC73C\uB85C \uB2F9\uBD84\uAC04 \uAE38\uB4DC\uC758 \uC7A1\uC77C\uC774\uB77C\uB3C4
      \uB3D5\uACE0 \uC2F6\uC740\uB370 \uC5B4\uB5A4\uAC00?"
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC74C\u2026 \uACA9\uBCC0 \uB54C\uBB38\uC5D0 \uAE38\uB4DC\uAC00 \uD55C\uC0B0\uD55C\uB370\u2026
      \uADF8\uB798\uB3C4 \uC728\uB9AC\uC6B0\uC2A4\uB2D8\uC758 \uB9C8\uC74C\uC744
      \uBB34\uC2DC\uD560 \uC218 \uC5C6\uC73C\uB2C8 \uAE38\uB4DC \uCC3D\uACE0 \uC815\uB9AC\uB97C
      \uAC19\uC774 \uD560\uAE4C\uC694?[n]\uBB3C\uAC74\uC774 \uC6CC\uB099 \uB9CE\uC544\uC11C
      \uD558\uB8E8 \uC774\uD2C0\uB85C\uB294 \uC548 \uB05D\uB0A0 \uAC81\uB2C8\uB2E4."
    speaker: {fileID: 11400000, guid: 8d960552c86c720459d7e1cef3afb705, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uADF8\uB798. \uAE38\uB4DC\uC758 \uCC3D\uACE0\uB77C. \uBB34\uC2A8 \uBB3C\uAC74\uB4E4\uC774
      \uC788\uC744\uC9C0 \uAD81\uAE08\uD558\uAD70."
    speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uADF8\uB7FC \uCC3D\uACE0\uB85C \uAC00\uBCFC\uAE4C\uC694! \uCC3D\uACE0\uB294
      \uC774 \uB4B7\uBB38\uC73C\uB85C \uAC00\uBA74\u2026"
    speaker: {fileID: 11400000, guid: 8d960552c86c720459d7e1cef3afb705, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 8d960552c86c720459d7e1cef3afb705, type: 2}
      spriteIndex: 0
      actionType: 3
      coords: {x: 0, y: 0}
      moveSpeed: 1
    - speaker: {fileID: 11400000, guid: 769a8b588781bf948bff6c93207bd827, type: 2}
      spriteIndex: 0
      actionType: 5
      coords: {x: 0, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC728\uB77C\uC6B0\uC2A4\uB2D8? \uC5B4\uB514 \uAC00\uC168\uC5B4\uC694?"
    speaker: {fileID: 11400000, guid: 8d960552c86c720459d7e1cef3afb705, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
