  sentences:
  - text: "\uC5B4\uC6B0\u2026 \uCC3E\uC558\uB2E4."
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
      spriteIndex: 0
      actionType: 1
      coords: {x: 400, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC5EC\uAE30\uAC00 \uB9DE\uC544 \uB8E8\uB4DC?"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
      spriteIndex: 0
      actionType: 1
      coords: {x: -600, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC5EC\uAE30 \uB9DE\uC544. \uD0A4\uC57C~ \uC774 \uBC29\uC73C\uB85C \uB4E4\uC5B4\uAC14\uB2E4\uAC00
      \uBBF8\uAD81\uC758 \uACA9\uBCC0\uC774 \uC77C\uC5B4\uB0A0 \uC904 \uBAB0\uB790\uC9C0."
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC74C\uC74C\u2026 \uBB38\uC744 \uBCF4\uBA74 \uB0B4\uAC00 \uD6D1\uC740
      \uD754\uC801\uC774 \uADF8\uB300\uB85C \uC788\uC5B4. \uACA9\uBCC0 \uC911 \uC774
      \uBC29\uC740 \uD1B5\uC9F8\uB85C \uC774\uB3D9\uD588\uB098 \uBCF4\uB124."
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uADF8\uB7FC \uBC14\uB85C \uB4E4\uC5B4\uAC00\uBCFC\uAE4C!"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC544\uB2C8 \uD50C\uB808\uC544. \uC77C\uB2E8 \uBB38 \uC55E\uC5D0 \uD754\uC801\uC744
      \uB0A8\uAE30\uACE0 \uB3CC\uC544\uAC00\uC790."
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC65C\uC5D0\u2026 \uC1E0\uBFD4\uB3C4 \uB2E8\uAE40\uC5D0 \uBE7C\uC790\uACE0\u266A"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uC548\uB3FC. \uC5EC\uAE30\uAE4C\uC9C0 \uC624\uBA74\uC11C \uCCB4\uB825\uC744
      \uB108\uBB34 \uB9CE\uC774 \uC37C\uC5B4."
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uADF8\uB9AC\uACE0 \uC774 \uBC29\uC740 \uBE4C\uC5B4\uBA39\uC744 \uACA9\uBCC0\uC774
      \uC2DC\uC791 \uB41C \uBC29\uC778\uB370. \uB2E8\uB2E8\uD788 \uC900\uBE44\uD558\uC9C0
      \uC54A\uC73C\uBA74 \uD070\uC77C \uB0A0 \uAC83 \uAC19\uC544."
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uB8E8\uB4DC\uAC00 \uADF8\uB807\uAC8C \uB9D0\uD55C\uB2E4\uBA74 \uC774
      \uC5D8\uD504\uB2D8\uAED8\uC11C \uB530\uB77C\uC904 \uC218 \uBC16\uC5D0 \uC5C6\uC9C0"
    speaker: {fileID: 11400000, guid: 1a686b430ab517342a58061e99772d16, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uBB50\uB798 \uBC18\uCABD\uC774 \uC5D8\uD504\uB3C4 \uC544\uB2CC \uAC8C"
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\uADF8\uB9CC \uB3CC\uC544\uAC00\uC790 \uD53C\uACE4\uD558\uB2E4."
    speaker: {fileID: 11400000, guid: aa14cce341d8c304faa4c53d65d18640, type: 2}
    actions: []
    music: {fileID: 0}
    sound: {fileID: 0}
